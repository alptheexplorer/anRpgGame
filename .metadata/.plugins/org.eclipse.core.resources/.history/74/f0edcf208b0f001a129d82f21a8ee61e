package game.tutos.area.tuto1;

import ch.epfl.cs107.play.game.tutos.area.SimpleArea;
import ch.epfl.cs107.play.math.Vector;
import game.tutos.actor.SimpleGhost;

public class Village extends SimpleArea{
	
	private SimpleGhost ghost;
	public void createArea() {
		this.ghost = new SimpleGhost(new Vector(18,7), "ghost.2");
		registerActor(ghost);
	}
	
	@Override
	public String getTitle() {
	return "zelda/Village";
	}

	
}
